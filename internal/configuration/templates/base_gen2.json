{
  "device": {
    "name": "{{.Device.Name}}",
    "mac": "{{.Device.MAC | macColon}}",
    "fw_id": "{{.Device.Firmware}}",
    "discoverable": true,
    "eco_mode": false,
    "debug": false
  },
  "sys": {
    "device": {
      "name": "{{.Device.Name}}",
      "mac": "{{.Device.MAC | macColon}}",
      "fw_id": "{{.Device.Firmware}}",
      "discoverable": true,
      "eco_mode": false
    },
    "location": {
      "tz": "UTC",
      "lat": 0,
      "lon": 0
    },
    "debug": {
      "level": 2,
      "file_level": null,
      "mqtt": {
        "enable": false
      },
      "websocket": {
        "enable": false
      },
      "udp": {
        "addr": null
      }
    },
    "ui_data": {},
    "rpc_udp": {
      "dst_addr": null,
      "listen_port": null
    },
    "sntp": {
      "server": "pool.ntp.org"
    }
  },
  "wifi": {
    "ap": {
      "enable": false,
      "ssid": "ShellyDevice",
      "pass": null,
      "is_open": true,
      "enable_on_disconnect": true,
      "range_extender": {
        "enable": false
      }
    },
    "sta": {
      "enable": true,
      "ssid": "{{.Network.SSID}}",
      "pass": "",
      "is_open": false,
      "enable": true,
      "ipv4mode": "dhcp",
      "ip": null,
      "netmask": null,
      "gw": null,
      "nameserver": null
    },
    "sta1": {
      "enable": false,
      "ssid": null,
      "pass": null,
      "is_open": true,
      "enable": true,
      "ipv4mode": "dhcp",
      "ip": null,
      "netmask": null,
      "gw": null,
      "nameserver": null
    },
    "roam": {
      "rssi_thr": -80,
      "interval": 60
    }
  },
  "mqtt": {
    "enable": false,
    "server": "mqtt.local:1883",
    "client_id": "{{.Device.MAC | macNone}}",
    "user": "",
    "pass": "",
    "ssl_ca": null,
    "topic_prefix": "shelly-device",
    "rpc_ntf": true,
    "status_ntf": false,
    "use_client_cert": false,
    "enable_rpc": true,
    "enable_control": true
  },
  "cloud": {
    "enable": false,
    "server": "shelly-103-eu.shelly.cloud:6022/jrpc"
  },
  "ws": {
    "enable": false,
    "server": null,
    "ssl_ca": "ca.pem"
  }
}